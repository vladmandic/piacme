{
  "globals": {},
  "env": {
      "browser": false,
      "commonjs": true,
      "es6": true,
      "node": true,
      "es2020": true
  },
  "parserOptions": { "ecmaVersion": 2020 },
  "extends": [
      "eslint:recommended",
      "airbnb-base"
  ],
  "ignorePatterns": [ "dist", "node_modules" ],
  "rules": {
    "max-len": [1, 200, 3],
    "radix": "off",
    "dot-notation": "off"
  }
}